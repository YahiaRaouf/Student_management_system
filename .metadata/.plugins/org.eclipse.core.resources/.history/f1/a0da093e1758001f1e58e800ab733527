#ifndef MANAGEFUNCS_H_
#define MANAGEFUNCS_H_

struct student {
	char name[50];
	int id;
	int age;
	float gpa;
};

struct node {
	struct student data;
	struct node *next;
};

void addStudent(const struct student *const students);
void enterData();
void displayStudents();


#endif /* MANAGEFUNCS_H_ */
